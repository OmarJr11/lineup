{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "projects": {
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "lineup": {
      "type": "application",
      "root": "apps/lineup",
      "entryFile": "main",
      "sourceRoot": "apps/lineup/src",
      "compilerOptions": {
        "tsConfigPath": "apps/lineup/tsconfig.app.json"
      }
    }
  }
}