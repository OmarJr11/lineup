{
  "name": "lineup-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:prod:admin": "rimraf dist/admin && tsc -p apps/admin/tsconfig.app.json",
    "build:prod:users": "rimraf dist/users && tsc -p apps/users/tsconfig.app.json",
    "build:prod:businesses": "rimraf dist/businesses && tsc -p apps/businesses/tsconfig.app.json",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:users": "nest start users",
    "start:dev:users": "nest start users --watch",
    "start:prod:users": "node dist/users/apps/users/src/main.js",
    "start:businesses": "nest start businesses",
    "start:dev:businesses": "nest start businesses --watch",
    "start:prod:businesses": "node dist/businesses/apps/businesses/src/main.js",
    "start:admin": "nest start admin",
    "start:dev:admin": "nest start admin --watch",
    "start:prod:admin": "node dist/admin/apps/admin/src/main.js",
    "start:all:prod": "concurrently \"npm run start:prod:users\" \"npm run start:prod:admin\" \"npm run start:prod:businesses\"",
    "start:all:dev": "concurrently \"npm run start:dev:users\" \"npm run start:dev:admin\" \"npm run start:dev:businesses\"",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:admin": "jest --config ./apps/admin/test/jest-e2e.json",
    "test:e2e:businesses": "jest --config ./apps/businesses/test/jest-e2e.json",
    "test:e2e:users": "jest --config ./apps/users/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./data-source.ts",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./data-source.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./data-source.ts",
    "migration:revert": "typeorm migration:revert -d ./data-source.ts",
    "migration:create": "typeorm migration:create"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@aws-sdk/client-s3": "^3.832.0",
    "@aws-sdk/s3-request-presigner": "^3.832.0",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "11.0.1",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/mapped-types": "2.1.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/typeorm": "11.0.0",
    "argon2": "0.43.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "concurrently": "^9.2.0",
    "dotenv": "16.5.0",
    "graphql": "^16.11.0",
    "joi": "^17.13.3",
    "moment": "2.30.1",
    "nestjs-typeorm-paginate": "^4.1.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pg": "8.16.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.1",
    "tsconfig-paths": "4.2.0",
    "typeorm": "0.3.24",
    "typeorm-transactional-cls-hooked": "0.1.21",
    "typescript-eslint": "8.33.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.18.0",
    "@nestjs/cli": "11.0.0",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.1",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.10.7",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/lodash": "^4.17.17",
    "@types/multer": "^1.4.13",
    "@types/node": "22.10.7",
    "@types/passport": "1.0.17",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.2",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.2",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
